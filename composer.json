{
    "name": "hungdx/mock-builder",
    "version": "1.0.0",
    "license": "MIT",
    "description": "The Mock builder for Laravel Framework. Use for unit test",
    "keywords": ["Mock Builder", "Mock Eloquent Query Builder", "Laravel Test", "laravel"],
    "homepage": "https://github.com/hugdx/mock-query-builder",
    "support": {
        "issues": "https://github.com/hugdx/mock-query-builder/issues",
        "source": "https://github.com/hugdx/mock-query-builder"
    },
    "authors": [
        {
            "name": "Xuan Hung",
            "email": "mr.hungdx@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0|^8",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7|^8|^9",
        "illuminate/support": "^5.7|^6|^7|^8|^9"
    },
    "autoload": {
        "psr-4": {
            "HungDX\\MockBuilder\\": "src/"
        }
    },
    "scripts": {
        "phpcs": "php-cs-fixer fix --dry-run",
        "phpcs-fix": "php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
